@model IEnumerable<ElectroShop.Models.MMenu>
<nav id="navigation">
    <div class="container">
        <div id="responsive-nav">
            <ul class="main-nav nav navbar-nav">
                <li class="active"><a href="~/">Trang chủ</a></li>
                @foreach (var row in Model)
                {
                    <li><a href="~/@row.Link">@row.Name</a></li>
                }
            </ul>
        </div>
    </div>
</nav>